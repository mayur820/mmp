
@{
    ViewBag.Title = "AcSwitch";
    Layout = "~/Views/Shared/_BLayout.cshtml";
}
<head>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/line-awesome/1.3.0/line-awesome/css/line-awesome.min.css">


    <script src="~/AngScripts/jquery-3.6.0.js"></script>
    <script src="~/AngScripts/jquery-3.6.0.min.js"></script>
    <script src="~/AngScripts/angular.js"></script>
    <script src="~/Scripts/jquery-3.5.1.min.js"></script>
    <script src="~/Scripts/popper.min.js"></script>
    <script src="~/Scripts/bootstrap.min.v4.js"></script>

    <link rel="stylesheet" href="~/css/bootstrap.min.v4.css">
    <link rel="stylesheet" href="~/css/style.css">
    <style>
        button.btn.btn-primary.btn-soft-primary.btn-lg.text-uppercase.text-center.ng-binding {
            width: 100%;
        }

       
    </style>
</head>
<body>

    <div ng-app="myApp" class="wrapper">
        <form action="" ng-controller="myctrn" ng-init="PageLoad()">
            <div class="text-center mb-4">
                <img src="~/images/logo.png" alt="IRecord" />
            </div>

            <div class="tab-content mt-4">
                <!-- 2 -->
                <div class="tab-pane fade active show" id="tab_2">
                    <h2 class="text-center mb-4">Account Switch</h2>

                    <div ng-repeat="data in Info" class="justify-content-center form-row mt-4 text-center">
                        <button ng-click="fnchangelogin(data)" class="btn btn-primary btn-soft-primary btn-lg text-uppercase text-center">Login as {{data.UserName}}</button>

                    </div>

                </div>
            </div>
        </form>
    </div>
    <script src="~/AngularDataFunction/AngularDataFunction.js"></script>
    <script src="~/AngScripts/AcSwitch.js"></script>
</body>

