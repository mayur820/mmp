                                <select class="form-control select2 select2-hidden-accessible" data-select2-id="4" tabindex="-1" aria-hidden="true">
                                    <option value="-1" data-select2-id="6">Select</option>
                                    <option value="AK">Broker 1</option>
                                    <option value="HI">Broker 2</option>
                                </select><span class="select2 select2-container select2-container--default" dir="ltr" data-select2-id="5" style="width: auto;"><span class="selection"><span class="select2-selection select2-selection--single" role="combobox" aria-haspopup="true" aria-expanded="false" tabindex="0" aria-disabled="false" aria-labelledby="select2-vgql-container"><span class="select2-selection__rendered" id="select2-vgql-container" role="textbox" aria-readonly="true" title="Select">Select</span><span class="select2-selection__arrow" role="presentation"><b role="presentation"></b></span></span></span><span class="dropdown-wrapper" aria-hidden="true"></span></span>
                            </div>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-holder">
                            <label for="">
                                Select Default Demat A/c:
                            </label>
                            <div class="d-flex">
                                <select class="form-control select2 select2-hidden-accessible" data-select2-id="7" tabindex="-1" aria-hidden="true">
                                    <option value="-1" data-select2-id="9">Select</option>
                                    <option value="AK">Demat 1</option>
                                    <option value="HI">Demat 2</option>
                                </select><span class="select2 select2-container select2-container--default" dir="ltr" data-select2-id="8" style="width: auto;"><span class="selection"><span class="select2-selection select2-selection--single" role="combobox" aria-haspopup="true" aria-expanded="false" tabindex="0" aria-disabled="false" aria-labelledby="select2-ovvd-container"><span class="select2-selection__rendered" id="select2-ovvd-container" role="textbox" aria-readonly="true" title="Select">Select</span><span class="select2-selection__arrow" role="presentation"><b role="presentation"></b></span></span></span><span class="dropdown-wrapper" aria-hidden="true"></span></span>
                            </div>
