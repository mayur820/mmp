<body data-select2-id="27">
    <div ng-app="myApp" ng-controller="myctrn" ng-init="PageLoad()" class="d-flex Login_box">
        <div class="cms_display_data d-flex align-items-center">
            <div class="w-100 text-center">
                <div class="left_banner owl-carousel owl-theme">
                    <div class="item">
                        <img src="~/images/slide-1.png"  alt="Slide 1" />
                    </div>
                    <div class="item">
                        <img src="~/images/slide-2.png" alt="Slide 2" />
                    </div>
                    <div class="item">
                        <img src="https://via.placeholder.com/500x500.png/007BFF/FFFFFF?text=Irecord+Ads+500x500" alt="">
                    </div>
                </div>
            </div>
        </div>
        <div class="cms_reg_form d-flex">
            <div class="wrapper" data-select2-id="26">
                <form action="https://mangeshirecord.github.io/HTML/on-board/index-2.html" data-select2-id="25">
                    <div class="text-center mb-4">
                        <img src="~/images/logo.png" alt="IRecord" />
                        
                        <h1 style="margin-bottom: 15px;">Member Login</h1>
                    </div>

                    <!-- <div class="checkout-wrap">
                        <ul class="checkout-bar nav_tabs d-flex">
                            <li class="active"><a href="#tab_1" data-toggle="tab"></a></li>
                            <li class=""><a href="#tab_2" data-toggle="tab"></a></li>
                        </ul>
                    </div> -->
                    <div class="" data-select2-id="24">
                        <div class="tabbable-panel" data-select2-id="23">
                            <div class="tabbable-line" data-select2-id="22">
                                <ul class="nav nav-tabs d-none">
                                    <li class="active"><a href="https://mangeshirecord.github.io/HTML/on-board/index-2.html#tab_1" data-toggle="tab">A</a></li>
                                    <li><a href="https://mangeshirecord.github.io/HTML/on-board/index-2.html#tab_2" data-toggle="tab">A</a></li>
                                </ul>
                                <div class="tab-content mt-3" data-select2-id="21">
                                    <!-- 1 -->
                                    <div class="tab-pane fade active show" id="tab_1" data-select2-id="tab_1">
                                        <div class="form-holder mb-3" data-select2-id="20">
                                            <label class="mb-0" for="">Select Member:</label>
                                            <div class="d-flex" data-select2-id="19">
                                                <select class="form-control select2" id="ddlmember" ng-change="onmemberchange()" ng-model="details.Member" ng-options="x.UserId as x.UserName for x in InfoMember">
                                                    <option ng-option value="-1">Select Member</option>

                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-holder" data-select2-id="36">
                                            <label class="mb-0" for="">Financial Year:</label>
                                            <div class="d-flex" data-select2-id="35">
                                                <div class="input-group tbl_int_grp" data-select2-id="34">
                                                    <select class="form-control select2" id="ddlyear" ng-model="details.selectyear" ng-options="x.ID as x.NAME for x in Info"  >
                                                       <option ng-option value="-1">Select Broker Financial Year</option>
                                     
                                                    </select>
                                                </div>
                                                <a ng-click="fnadd()" class="d-flex align-items-center no_wrap btn btn-primary ms-2">
                                                    + Add
                                                </a>
                                            </div>
                                        </div>
                                        <div class="table-responsive mt-3">
                                            <table class="table table-sm table-bordered">
                                                <thead class="table-light">
                                                    <tr>
                                                        <th>Sr No</th>
                                                        <th>Code</th>
                                                        <th>From Date</th>
                                                        <th>To Date</th>
                                                        <th>Actions</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="datatable in InfoAllYear">
                                                        <td>{{$index+1}}</td>
                                                        <td>{{datatable.Code}} </td>
                                                        <td>{{datatable.FromDate}}</td>
                                                        <td>{{datatable.ToDate}}</td>
                                                        <td>
                                                            <a ng-click="fnsetFinancialYear(datatable)" class="no_wrap btn btn-outline-primary btn-sm">
                                                                Login →
                                                            </a>
                                                        </td>
                                                    </tr>
                                                  
                                                </tbody>
                                            </table>
                                        </div>


                                    </div>


                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <!-- End -->
        </div>
    </div>


