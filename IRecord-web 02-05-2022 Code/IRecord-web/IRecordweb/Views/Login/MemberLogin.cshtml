    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @*<meta name="author" content="colorlib.com">*@

    <link rel="stylesheet" href="~/css/line-awesome.min.css">
    <link rel="stylesheet" href="~/css/bootstrap.min.v4.css">
