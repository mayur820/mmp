<div ng-app="myApp" ng-controller="myctrn" ng-init="PageLoad()" id="" class="">
    <div id="layout-wrapper">

        <!-- ========== Left Sidebar Start ========== -->
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->



        <div class="container-fluid">

            <div class="display_adds display_adds_top text-center mb-3">
                <img src="https://via.placeholder.com/970x90.png/007BFF/FFFFFF?text=Irecord+Ads" alt="" />
            </div>

            <!-- start page title -->
            <div class="row">
                <div class="col-12">
                    <div class="page-title-box d-flex align-items-center justify-content-between">
                        <h4 class="mb-0">Broker Bill</h4>

                        <div class="page-title-right">
                            <div class="dropdown">
                                <button class="btn btn-primary dropdown-toggle" type="button" id="dropdownMenuButton" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <i class="las la-plus font-size-18"></i> Add New <i class="las la-angle-down"></i>
                                </button>
                                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuButton">
                                    <a class="dropdown-item" href="/BrokerBillEntry/SaveBREntry">Import</a>
                                    <a class="dropdown-item" href="/BrokerBillEntry/ManualEntry">Manual Entry</a>
                                </div>
                            </div>
                            <!-- <a href="add.html" class="px-2 btn btn-primary"><i class="las la-plus font-size-18"></i> Add New</a> -->
                        </div>

                    </div>
                </div>
            </div>
            <!-- end page title -->
            <!-- Working Area Start -->
            <div class="card">
                <div class="card-body">
                    <div style="display:none" class="search_filter mb-3">
                        <div class="row">
                            <div class="col-sm-12 col-md-4 mb-3 d-flex flex-column justify-content-end">
                                <label class="mb-0">Datewise Bill:</label>

                            </div>

                            <div class="col-sm-12 col-md-2 mb-3 align-self-end">
                                <input type="submit" class="btn btn-primary" value="Search">
                            </div>
                        </div>
                    </div>

                    <div class="pagination_box w-100 mb-3 d-sm-flex justify-content-end">
                        <div class="float-sm-end">
                            <uib-pagination class="pagination MYPAGECSS" total-items="Info.length" ng-model="page"
                                            ng-change="pageChanged()" previous-text="&lsaquo;" next-text="&rsaquo;" items-per-page=10></uib-pagination>
                        </div>
                    </div>
                    <div class="table-responsive">
                        <table class="table table-hover table-bordered mb-0">
                            <thead class="table-light">
                                <tr>
                                    <th>#</th>
                                    <th>Date <input type="text" class="form-control datepicker" ng-model="search.Date" placeholder="DD-MM-YYYY" data-date-format="dd-mm-yyyy"></th>
                                    <th>Bill No.<input type="text" class="form-control" ng-model="search.Bill_no" placeholder="Bill Number"></th>
                                    <th>Settlement No<input type="text" class="form-control " ng-model="search.SettlementNo" placeholder="Settlement No"></th>
                                    <th>Broker Name <input type="text" class="form-control " ng-model="search.BrokerName" placeholder="Broker Name"></th>
                                    <!-- <th>Brokerage</th>
                                    <th>Other Exp.</th> -->
                                    <th>Net Amount <input type="text" class="form-control " ng-model="search.Net_Amount" placeholder="Net Amount"></th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>

                                <tr ng-repeat="data in Info | filter:search | limitTo:10:10*(page-1)">
                                    <td>{{$index+1}}</td>
                                    <td>{{data.Date}}</td>
                                    <td>{{data.Bill_no}}</td>
                                    <td>{{data.SettlementNo}}</td>
                                    <td>{{data.BrokerName}}</td>

                                    <td><a class="btn btn-sm btn-outline-primary" href="javascript:;" />{{data.Net_Amount}}</td>
                                    <td class="whitespace_nowrap">
                                        <ul class="list-inline mb-0">
                                            <li class="list-inline-item">
                                                <a href="/BrokerBillEntry/ViewCnNote?TrnsNo={{data.Trans_no}}&&Status=View" class="px-2 btn btn-outline-primary"><i class="las la-search font-size-18"></i></a>
                                            </li>
                                            <li class="list-inline-item">
                                                <a href="/BrokerBillEntry/ViewCnNote?TrnsNo={{data.Trans_no}}&&Status=Edit" class="px-2 btn btn-outline-primary"><i class="las la-pencil-alt font-size-18"></i></a>
                                            </li>
                                            <li class="list-inline-item">
                                                <a href="javascript:void(0);" ng-click="Isdeleted(data)" class="px-2 btn btn-outline-danger"><i class="las la-trash-alt font-size-18"></i></a>
                                            </li>
                                        </ul>
                                    </td>
                                </tr>

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <!-- Working Area End -->

        </div> <!-- container-fluid -->
        <!-- End Page-content -->


        <footer class="footer">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-sm-6">
                        <script>document.write(new Date().getFullYear())</script> <i class="las la-copyright"></i> iRecord.
                    </div>
                    <!-- <div class="col-sm-6">
                        <div class="text-sm-end d-none d-sm-block">
                            Crafted with <i class="mdi mdi-heart text-danger"></i> by <a href="https://themesbrand.com/" target="_blank" class="text-reset">Themesbrand</a>
                        </div>
                    </div> -->
                </div>
            </div>
        </footer>

        <!-- end main content-->

    </div>
    <!-- END layout-wrapper -->
    <!-- Right bar overlay-->
    <div class="rightbar-overlay"></div>


    <!-- Modal 1 -->
    <div class="modal fade" id="modal_add" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable modal-xl">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="staticBackdropLabel">Add Consultant Master</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <div class="row form_2">
                        <div class="col-sm-12 col-md-6 mb-3 d-flex">
                            <label class="mb-0">Consultant Name <span class="text-danger">*</span>:</label>
                            <input type="text" id="family_name" name="family_name" class="form-control" placeholder="Enter Consultant Name" />
                        </div>
                    </div>
                </div>
                <div class="modal-footer justify-content-center">
                    <button type="button" class="btn btn-primary" data-bs-dismiss="modal">Save</button>
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Reset</button>
                </div>
            </div>
        </div>
    </div>
</div>

