                    <div class="row form_2 view_mode">
                        <div class="col-sm-12 col-md-6 mb-3 d-flex">
                            <label class="mb-0">Member Name:</label>
                            <span class="form-control"><label ng-model="Info.MemberName">@Session["MemberName"].ToString()</label></span>
                        </div>
                        <div class="col-sm-12 col-md-6 mb-3 d-flex">
                            <label class="mb-0">Date:</label>
                            <span class="form-control"><label ng-model="Info.Date" />{{Info.Date}}</span>
                        </div>
                        <div class="col-sm-12 col-md-6 mb-3 d-flex">
                            <label class="mb-0">Settlement No:</label>
                            <span class="form-control"><label ng-model="Info.SettlementNo" />{{Info.SettlementNo}}</span>
                        </div>
                        <div class="col-sm-12 col-md-6 mb-3 d-flex">
                            <label class="mb-0">Bill No:</label>
                            <span class="form-control"><label ng-model="Info.BillNo" />{{Info.BillNo}}</span>
                        </div>
                        <div class="col-sm-12 col-md-6 mb-3 d-flex">
                            <label class="mb-0">Broker:</label>
                            <span class="form-control"><label ng-model="Info.BrokerName" />{{Info.BrokerName}}</span>
                        </div>
