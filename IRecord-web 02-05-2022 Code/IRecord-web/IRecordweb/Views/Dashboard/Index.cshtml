@using System.Data;
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/js/jquery-3.5.1.min.js"></script>
<script src="~/js/popper.min.js"></script>
<script src="~/js/bootstrap.min.js"></script>
<script src="~/js/metisMenu.min.js"></script>
<script src="~/js/simplebar.min.js"></script>
<script src="~/js/app.js"></script>
<script src="~/js/amchart/core.js"></script>
<script src="~/js/amchart/charts.js"></script>
<script src="~/js/amchart/animated.js"></script>
<script src="~/js/amchart/chart-data-1.js"></script>
<script src="~/js/amchart/chart-data-2.js"></script>
<script src="~/js/amchart/chart-data-3.js"></script>
<script src="~/js/custom-script.js"></script>
<script type="/javascript">
    function addLoadEvent(func) {
        var oldonload = window.onload;
        if (typeof window.onload != 'function') {
            window.onload = func;
        } else {
            window.onload = function () {
                if (oldonload) {
                    oldonload();
                }
            }
        }
    }
    addLoadEvent(setmenu('1'));
</script>
@if (Session["Role_Type"].ToString() != "Member")
{
    <div class="row">
        <div class="col-12">
            <div class="page-title-box d-flex align-items-center justify-content-between">
                <h4 class="mb-0">Dashboard</h4>
                <div class="page-title-right">

                </div>
            </div>
        </div>
    </div>
    <!-- end page title -->
    <div class="card dashboard_par">
        <div class="card-body">
            <div class="top_member_selection">
                <div class="row">
                    <div class="col">
                        <div class="mt-1 mb-1 form-group d-flex flex-column">
                            <label class="mb-0">Select Member:</label>
                            <select class="form-select">
                                <option>-- Select Member --</option>
                                <option>Member 1</option>
                                <option>Member 2</option>
                                <option>Member 3</option>
                            </select>
                        </div>
                    </div>
                    <div class="col">
                        <div class="mt-1 mb-1 form-group d-flex flex-column">
                            <label class="mb-0">Select Consultant4:</label>
                            <select class="form-select">
                                <option>-- Select Consultant4 --</option>
                                <option>Consultant4 1</option>
                                <option>Consultant4 2</option>
                                <option>Consultant4 3</option>
                            </select>
                        </div>
                    </div>
                    <div class="col d-flex align-items-end submit_div mb-1">
                        <input type="submit" value="Submit" class="btn btn-primary" />
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <div class="card dashboard_par">
                <div class="card-body">
                    <h2 class="card-title">Licensee Details</h2>
                    <div class="table-responsive">
                        <table class="table table-striped mb-0">
                            <thead>

                                <tr>
                                    <th scope="col">Subscriber Name</th>
                                    <th scope="col">No Of License</th>
                                    <th scope="col">Start Date</th>
                                    <th scope="col">End Date</th>
                                    <th scope="col">Payment Status</th>
                                    <th scope="col">Amount </th>
                                    <th scope="col">License Qty</th>
                                    <th scope="col">Coupon Code </th>
                                    <th scope="col">Payment Date</th>
                                </tr>
                            </thead>
                            <tbody>
                                @{
                                DataTable dtlinc = Session["DDTDisplayAllStatus"] as DataTable;
                                foreach (DataRow dr in dtlinc.Rows)
                                {
                                        <tr>
                                            <th scope="row">@dr["SubscriberName"].ToString()</th>
                                            <td>@dr["NoOfLicense"].ToString()</td>

                                            <td>@dr["StartDate"].ToString()</td>
                                            <td>@dr["EndDate"].ToString()</td>
                                            <td>@dr["PaymentStatus"].ToString()</td>
                                            <td>@dr["PAYMENT_AMOUNT"].ToString()</td>
                                            <td>@dr["LICENSE_QTY"].ToString()</td>
                                            <td>@dr["COUPON_CODE"].ToString()</td>
                                            <td>@dr["PAYMENT_DATE"].ToString()</td>


                                        </tr>
                                }
                                }


                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-sm-12">
            <div class="card dashboard_par">
                <div class="card-body">
                    <h2 class="card-title">All User Reports</h2>

                    <div class="table-responsive">
                        <table class="table table-striped mb-0">
                            <thead>
                                <tr>
                                    <th scope="col">#</th>
                                    <th scope="col">Subscriber Name</th>
                                    <th scope="col">Operator Name</th>
                                    <th scope="col">Member Name</th>

                                </tr>
                            </thead>
                            <tbody>
                                @{
                                int rowcounter = 1;
                                DataTable dtuser = Session["DDTDisplayAllUser"] as DataTable;
                                foreach (DataRow dr in dtuser.Rows)
                                {
                            <tr>
                                <th scope="row">@rowcounter.ToString()</th>
                                <th scope="row">@dr["SubscriberName"].ToString()</th>
                                <td>@dr["OperatorName"].ToString()</td>

                                <td>@dr["memberName"].ToString()</td>



                            </tr>
                                    rowcounter++;
                                }
                                }

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-sm-12 col-md-6">


            <div class="card dashboard_par">
                <div class="card-body chart_div">
                    <div class="d-flex justify-content-between mb-3">
                        <h4 class="mb-1">Market Value - <span>1,00,000</span></h4>
                        <a href="~/Dashboard/Dashboardmarket" class="whitespace_nowrap align-self-start btn btn-primary btn-sm mb-1">View All</a>
                    </div>
                    <div class="chart_box" id="chartdiv_1"></div>
                </div>
            </div>
        </div>
        <div class="col-sm-12 col-md-6">
            <div class="card dashboard_par">
                <div class="card-body chart_div">
                    <div class="d-flex justify-content-between mb-3">
                        <h4 class="mb-1">Long Term - <span>64,000</span></h4>
                        <a href="~/Dashboard/Dashboardlongterm" class="whitespace_nowrap align-self-start btn btn-primary btn-sm mb-1">View All</a>
                    </div>
                    <div class="chart_box" id="chartdiv_2"></div>
                </div>
            </div>
        </div>
        <div class="col-sm-12 col-md-6">
            <div class="card dashboard_par">
                <div class="card-body chart_div">
                    <div class="d-flex justify-content-between mb-3">
                        <h4 class="mb-1">Short Term - <span>36,000</span></h4>
                        <a href="~/Dashboard/DashboardShortTerm" class="whitespace_nowrap align-self-start btn btn-primary btn-sm mb-1">View All</a>
                    </div>
                    <div class="chart_box" id="chartdiv_3"></div>
                </div>
            </div>
        </div>
    </div>

}
